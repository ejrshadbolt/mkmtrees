	"scripts": {
		"dev": "next dev",
		"dev:local": "npm run build:cf && DB_NAME=$(node scripts/get-db-name.js) && COMPAT_DATE=$(node scripts/get-compatibility-date.js) && wrangler pages dev .vercel/output/static --compatibility-date=$COMPAT_DATE --d1=DB:$DB_NAME --port=3000 --local",
		"build": "next build",
		"build:cf": "next build && npx @cloudflare/next-on-pages",
		"start": "next start",
		"lint": "next lint",
		"deploy": "npm run build:cf && wrangler pages deploy .vercel/output/static",
		"preview": "npm run build:cf && COMPAT_DATE=$(node scripts/get-compatibility-date.js) && wrangler pages dev .vercel/output/static --compatibility-date=$COMPAT_DATE",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"db:schema": "node ./scripts/init-db.js",
		"db:create-admin": "node ./scripts/create-admin.js"
	},
